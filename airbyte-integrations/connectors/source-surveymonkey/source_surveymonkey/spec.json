{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/surveymonkey",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "PostHog Spec",
    "type": "object",
    "required": ["start_date", "access_token"],
    "additionalProperties": false,
    "properties": {
      "start_date": {
        "title": "Start Date",
        "type": "string",
        "description": "The date from which you'd like to replicate the data",
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$",
        "examples": "2021-01-01T00:00:00Z"
      },
      "access_token": {
        "type": "string",
        "airbyte_secret": true,
        "description": "API Token. See the <a href=\"https://docs.airbyte.io/integrations/sources/surveymonkey\">docs</a> for information on how to generate this key."
      },
      "objects_to_replicate": {
        "type": "object",
        "title": "Surveys to replicate",
        "description": "Replicate all surveys or some of them?",
        "oneOf": [
          {
            "title": "Replicate all surveys",
            "additionalProperties": false,
            "required": ["method"],
            "properties": {
              "method": {
                "type": "string",
                "enum": ["replicate_all"],
                "default": "replicate_all"
              }
            }
          },
          {
            "title": "Replicate some surveys",
            "additionalProperties": false,
            "required": ["method", "survey_ids"],
            "properties": {
              "method": {
                "type": "string",
                "enum": ["replicate_some"],
                "default": "replicate_some"
              },
              "survey_ids": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        ]
      }
    }
  }
}
